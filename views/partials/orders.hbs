<table id="content-table" data-key="id" data-update_rows="true" data-table="orders">
<tr><th>#</th><th>id</th><th>accepted_bidder</th><th>bid_end_time</th><th>source_mob</th><th>dest_mob</th><th>source_latitude</th><th>source_longitude</th><th>source_location</th><th>dest_latitude</th><th>dest_longitude</th><th>dest_location</th><th>geo_hash</th><th>goods_desc</th><th>dimen_length</th><th>dimen_width</th><th>dimen_height</th><th>load_time</th><th>load_duration</th><th>customer_id</th><th>num_vehicles</th><th>cancelled_by</th><th>unique_key</th><th>payment_mode</th><th>restart_bid_count</th><th>status</th><th>unload_duration</th><th>vehicle</th><th>vehicle_category</th><th>goods_weight</th><th>weight_unit</th><th>creation_timestamp</th><th>accept_timestamp</th><th>completion_timestamp</th><th>cancellation_timestamp</th><th>net_amount</th><th>amount_paid</th><th>rating</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="int"><span>{{id}}</span></td><td data-update="true" data-col="accepted_bidder" data-dtype="int"><span>{{accepted_bidder}}</span></td><td data-update="true" data-col="bid_end_time" data-dtype="bigint"><span>{{bid_end_time}}</span></td><td data-update="true" data-col="source_mob" data-dtype="varchar(15)"><span>{{source_mob}}</span></td><td data-update="true" data-col="dest_mob" data-dtype="varchar(15)"><span>{{dest_mob}}</span></td><td data-update="true" data-col="source_latitude" data-dtype="double"><span>{{source_latitude}}</span></td><td data-update="true" data-col="source_longitude" data-dtype="double"><span>{{source_longitude}}</span></td><td data-update="true" data-col="source_location" data-dtype="text"><span>{{source_location}}</span></td><td data-update="true" data-col="dest_latitude" data-dtype="double"><span>{{dest_latitude}}</span></td><td data-update="true" data-col="dest_longitude" data-dtype="double"><span>{{dest_longitude}}</span></td><td data-update="true" data-col="dest_location" data-dtype="text"><span>{{dest_location}}</span></td><td data-update="true" data-col="geo_hash" data-dtype="bigint"><span>{{geo_hash}}</span></td><td data-update="true" data-col="goods_desc" data-dtype="text"><span>{{goods_desc}}</span></td><td data-update="true" data-col="dimen_length" data-dtype="smallint"><span>{{dimen_length}}</span></td><td data-update="true" data-col="dimen_width" data-dtype="smallint"><span>{{dimen_width}}</span></td><td data-update="true" data-col="dimen_height" data-dtype="smallint"><span>{{dimen_height}}</span></td><td data-update="true" data-col="load_time" data-dtype="bigint"><span>{{load_time}}</span></td><td data-update="true" data-col="load_duration" data-dtype="float"><span>{{load_duration}}</span></td><td data-update="true" data-col="customer_id" data-dtype="int"><span>{{customer_id}}</span></td><td data-update="true" data-col="num_vehicles" data-dtype="int"><span>{{num_vehicles}}</span></td><td data-update="true" data-col="cancelled_by" data-dtype="int"><span>{{cancelled_by}}</span></td><td data-update="true" data-col="unique_key" data-dtype="bigint"><span>{{unique_key}}</span></td><td data-update="true" data-col="payment_mode" data-dtype="int"><span>{{payment_mode}}</span></td><td data-update="true" data-col="restart_bid_count" data-dtype="int"><span>{{restart_bid_count}}</span></td><td data-update="true" data-col="status" data-dtype="int"><span>{{status}}</span></td><td data-update="true" data-col="unload_duration" data-dtype="float"><span>{{unload_duration}}</span></td><td data-update="true" data-col="vehicle" data-dtype="varchar(50)"><span>{{vehicle}}</span></td><td data-update="true" data-col="vehicle_category" data-dtype="varchar(20)"><span>{{vehicle_category}}</span></td><td data-update="true" data-col="goods_weight" data-dtype="int"><span>{{goods_weight}}</span></td><td data-update="true" data-col="weight_unit" data-dtype="varchar(5)"><span>{{weight_unit}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="bigint"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="accept_timestamp" data-dtype="bigint"><span>{{accept_timestamp}}</span></td><td data-update="true" data-col="completion_timestamp" data-dtype="bigint"><span>{{completion_timestamp}}</span></td><td data-update="true" data-col="cancellation_timestamp" data-dtype="bigint"><span>{{cancellation_timestamp}}</span></td><td data-update="true" data-col="net_amount" data-dtype="int"><span>{{net_amount}}</span></td><td data-update="true" data-col="amount_paid" data-dtype="int"><span>{{amount_paid}}</span></td><td data-update="true" data-col="rating" data-dtype="float"><span>{{rating}}</span></td></tr>{{/each}}
</table>