<table id="content-table" data-key="vehicle_number" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="vehicles">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="vehicle_number" data-dtype="text" data-default="null" data-nullable="false">vehicle_number</th><th data-insert="true" data-allow_update="true" data-col="vehicle" data-dtype="text" data-default="null" data-nullable="false">vehicle</th><th data-insert="true" data-allow_update="true" data-col="verified" data-dtype="boolean" data-default="0" data-nullable="true">verified</th><th data-insert="true" data-allow_update="true" data-col="owner" data-dtype="number" data-default="null" data-nullable="false">owner</th><th data-insert="true" data-allow_update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">creation_timestamp</th><th data-insert="true" data-allow_update="true" data-col="verification_timestamp" data-dtype="unixtimestamp" data-default="0" data-nullable="true">verification_timestamp</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="vehicle_number" data-dtype="text" class="dt-text"><span>{{vehicle_number}}</span></td><td data-update="true" data-col="vehicle" data-dtype="text" class="dt-text"><span>{{vehicle}}</span></td><td data-update="true" data-col="verified" data-dtype="boolean" class="dt-boolean"><span>{{verified}}</span></td><td data-update="true" data-col="owner" data-dtype="number" class="dt-number"><span>{{owner}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="verification_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{verification_timestamp}}</span></td></tr>{{/each}}
</table>