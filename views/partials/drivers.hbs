<table id="content-table" data-key="id" data-update_rows="true" data-table="drivers">
<tr><th>#</th><th>id</th><th>name</th><th>mobile</th><th>email</th><th>company</th><th>current_trip</th><th>location</th><th>latitude</th><th>longitude</th><th>docs_mask</th><th>balance</th><th>verified</th><th>creation_timestamp</th><th>verification_timestamp</th><th>code</th><th>code_exp_time</th><th>transporter_id</th><th>enabled</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="int"><span>{{id}}</span></td><td data-update="true" data-col="name" data-dtype="varchar(30)"><span>{{name}}</span></td><td data-update="true" data-col="mobile" data-dtype="varchar(15)"><span>{{mobile}}</span></td><td data-update="true" data-col="email" data-dtype="varchar(30)"><span>{{email}}</span></td><td data-update="true" data-col="company" data-dtype="varchar(100)"><span>{{company}}</span></td><td data-update="true" data-col="current_trip" data-dtype="int"><span>{{current_trip}}</span></td><td data-update="true" data-col="location" data-dtype="text"><span>{{location}}</span></td><td data-update="true" data-col="latitude" data-dtype="double"><span>{{latitude}}</span></td><td data-update="true" data-col="longitude" data-dtype="double"><span>{{longitude}}</span></td><td data-update="true" data-col="docs_mask" data-dtype="int"><span>{{docs_mask}}</span></td><td data-update="true" data-col="balance" data-dtype="int"><span>{{balance}}</span></td><td data-update="true" data-col="verified" data-dtype="tinyint(1)"><span>{{verified}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="bigint"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="verification_timestamp" data-dtype="bigint"><span>{{verification_timestamp}}</span></td><td data-update="true" data-col="code" data-dtype="int"><span>{{code}}</span></td><td data-update="true" data-col="code_exp_time" data-dtype="bigint"><span>{{code_exp_time}}</span></td><td data-update="true" data-col="transporter_id" data-dtype="int"><span>{{transporter_id}}</span></td><td data-update="true" data-col="enabled" data-dtype="tinyint(1)"><span>{{enabled}}</span></td></tr>{{/each}}
</table>