<table id="content-table" data-key="id" data-update_rows="true" data-table="customers">
<tr><th>#</th><th>id</th><th>name</th><th>mobile</th><th>email</th><th>company</th><th>location</th><th>latitude</th><th>longitude</th><th>creation_timestamp</th><th>balance</th><th>enabled</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="int"><span>{{id}}</span></td><td data-update="true" data-col="name" data-dtype="varchar(30)"><span>{{name}}</span></td><td data-update="true" data-col="mobile" data-dtype="varchar(15)"><span>{{mobile}}</span></td><td data-update="true" data-col="email" data-dtype="varchar(30)"><span>{{email}}</span></td><td data-update="true" data-col="company" data-dtype="varchar(100)"><span>{{company}}</span></td><td data-update="true" data-col="location" data-dtype="text"><span>{{location}}</span></td><td data-update="true" data-col="latitude" data-dtype="double"><span>{{latitude}}</span></td><td data-update="true" data-col="longitude" data-dtype="double"><span>{{longitude}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="bigint"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="balance" data-dtype="int"><span>{{balance}}</span></td><td data-update="true" data-col="enabled" data-dtype="tinyint(1)"><span>{{enabled}}</span></td></tr>{{/each}}
</table>