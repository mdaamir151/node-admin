<table id="content-table" data-key="id" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="customers">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="id" data-dtype="number" data-default="null" data-nullable="false">id</th><th data-insert="true" data-allow_update="true" data-col="name" data-dtype="text" data-default="null" data-nullable="false">name</th><th data-insert="true" data-allow_update="true" data-col="mobile" data-dtype="text" data-default="null" data-nullable="false">mobile</th><th data-insert="true" data-allow_update="true" data-col="email" data-dtype="text" data-default="null" data-nullable="true">email</th><th data-insert="true" data-allow_update="true" data-col="company" data-dtype="text" data-default="null" data-nullable="true">company</th><th data-insert="true" data-allow_update="true" data-col="location" data-dtype="text" data-default="null" data-nullable="true">location</th><th data-insert="true" data-allow_update="true" data-col="latitude" data-dtype="number" data-default="0" data-nullable="true">latitude</th><th data-insert="true" data-allow_update="true" data-col="longitude" data-dtype="number" data-default="0" data-nullable="true">longitude</th><th data-insert="true" data-allow_update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">creation_timestamp</th><th data-insert="true" data-allow_update="true" data-col="balance" data-dtype="number" data-default="0" data-nullable="true">balance</th><th data-insert="true" data-allow_update="true" data-col="enabled" data-dtype="boolean" data-default="1" data-nullable="true">enabled</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="number" class="dt-number"><span>{{id}}</span></td><td data-update="true" data-col="name" data-dtype="text" class="dt-text"><span>{{name}}</span></td><td data-update="true" data-col="mobile" data-dtype="text" class="dt-text"><span>{{mobile}}</span></td><td data-update="true" data-col="email" data-dtype="text" class="dt-text"><span>{{email}}</span></td><td data-update="true" data-col="company" data-dtype="text" class="dt-text"><span>{{company}}</span></td><td data-update="true" data-col="location" data-dtype="text" class="dt-text"><span>{{location}}</span></td><td data-update="true" data-col="latitude" data-dtype="number" class="dt-number"><span>{{latitude}}</span></td><td data-update="true" data-col="longitude" data-dtype="number" class="dt-number"><span>{{longitude}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="balance" data-dtype="number" class="dt-number"><span>{{balance}}</span></td><td data-update="true" data-col="enabled" data-dtype="boolean" class="dt-boolean"><span>{{enabled}}</span></td></tr>{{/each}}
</table>