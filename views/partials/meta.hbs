<table id="content-table" data-key="id" data-update_rows="true" data-table="meta">
<tr><th>#</th><th>id</th><th>min_supported_customer_app</th><th>min_supported_partner_app</th><th>min_supported_driver_app</th><th>customer_cancellation_rate</th><th>transporter_cancellation_rate</th><th>gst_rate</th><th>service_rate</th><th>config_last_updated</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="int"><span>{{id}}</span></td><td data-update="true" data-col="min_supported_customer_app" data-dtype="double(10,2)"><span>{{min_supported_customer_app}}</span></td><td data-update="true" data-col="min_supported_partner_app" data-dtype="double(10,2)"><span>{{min_supported_partner_app}}</span></td><td data-update="true" data-col="min_supported_driver_app" data-dtype="double(10,2)"><span>{{min_supported_driver_app}}</span></td><td data-update="true" data-col="customer_cancellation_rate" data-dtype="double(10,2)"><span>{{customer_cancellation_rate}}</span></td><td data-update="true" data-col="transporter_cancellation_rate" data-dtype="double(10,2)"><span>{{transporter_cancellation_rate}}</span></td><td data-update="true" data-col="gst_rate" data-dtype="double(10,2)"><span>{{gst_rate}}</span></td><td data-update="true" data-col="service_rate" data-dtype="double(10,2)"><span>{{service_rate}}</span></td><td data-update="true" data-col="config_last_updated" data-dtype="bigint"><span>{{config_last_updated}}</span></td></tr>{{/each}}
</table>