<table id="content-table" data-key="id" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="saved_places">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="id" data-dtype="number" data-default="null" data-nullable="false">id</th><th data-insert="true" data-allow_update="true" data-col="address" data-dtype="text" data-default="null" data-nullable="false">address</th><th data-insert="true" data-allow_update="true" data-col="latitude" data-dtype="number" data-default="null" data-nullable="false">latitude</th><th data-insert="true" data-allow_update="true" data-col="longitude" data-dtype="number" data-default="null" data-nullable="false">longitude</th><th data-insert="true" data-allow_update="true" data-col="saved_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">saved_timestamp</th><th data-insert="true" data-allow_update="true" data-col="transporter_id" data-dtype="number" data-default="null" data-nullable="false">transporter_id</th><th data-insert="true" data-allow_update="true" data-col="notify" data-dtype="boolean" data-default="0" data-nullable="true">notify</th><th data-insert="true" data-allow_update="true" data-col="geo_hash" data-dtype="number" data-default="0" data-nullable="true">geo_hash</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="number" class="dt-number"><span>{{id}}</span></td><td data-update="true" data-col="address" data-dtype="text" class="dt-text"><span>{{address}}</span></td><td data-update="true" data-col="latitude" data-dtype="number" class="dt-number"><span>{{latitude}}</span></td><td data-update="true" data-col="longitude" data-dtype="number" class="dt-number"><span>{{longitude}}</span></td><td data-update="true" data-col="saved_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{saved_timestamp}}</span></td><td data-update="true" data-col="transporter_id" data-dtype="number" class="dt-number"><span>{{transporter_id}}</span></td><td data-update="true" data-col="notify" data-dtype="boolean" class="dt-boolean"><span>{{notify}}</span></td><td data-update="true" data-col="geo_hash" data-dtype="number" class="dt-number"><span>{{geo_hash}}</span></td></tr>{{/each}}
</table>