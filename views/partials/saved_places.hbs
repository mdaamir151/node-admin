<table id="content-table" data-key="id" data-update_rows="true" data-table="saved_places">
<tr><th>#</th><th>id</th><th>address</th><th>latitude</th><th>longitude</th><th>saved_timestamp</th><th>transporter_id</th><th>notify</th><th>geo_hash</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="int"><span>{{id}}</span></td><td data-update="true" data-col="address" data-dtype="varchar(50)"><span>{{address}}</span></td><td data-update="true" data-col="latitude" data-dtype="double"><span>{{latitude}}</span></td><td data-update="true" data-col="longitude" data-dtype="double"><span>{{longitude}}</span></td><td data-update="true" data-col="saved_timestamp" data-dtype="bigint"><span>{{saved_timestamp}}</span></td><td data-update="true" data-col="transporter_id" data-dtype="int"><span>{{transporter_id}}</span></td><td data-update="true" data-col="notify" data-dtype="tinyint(1)"><span>{{notify}}</span></td><td data-update="true" data-col="geo_hash" data-dtype="bigint"><span>{{geo_hash}}</span></td></tr>{{/each}}
</table>