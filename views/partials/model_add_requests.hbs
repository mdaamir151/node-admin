<table id="content-table" data-key="id" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="model_add_requests">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="id" data-dtype="number" data-default="null" data-nullable="false">id</th><th data-insert="true" data-allow_update="true" data-col="model" data-dtype="text" data-default="null" data-nullable="false">model</th><th data-insert="true" data-allow_update="true" data-col="transporter" data-dtype="number" data-default="null" data-nullable="false">transporter</th><th data-insert="true" data-allow_update="true" data-col="status" data-dtype="number" data-default="0" data-nullable="true">status</th><th data-insert="true" data-allow_update="true" data-col="request_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">request_timestamp</th><th data-insert="true" data-allow_update="true" data-col="conclusion_timestamp" data-dtype="unixtimestamp" data-default="0" data-nullable="true">conclusion_timestamp</th><th data-insert="true" data-allow_update="true" data-col="conclusion_text" data-dtype="text" data-default="null" data-nullable="true">conclusion_text</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="number" class="dt-number"><span>{{id}}</span></td><td data-update="true" data-col="model" data-dtype="text" class="dt-text"><span>{{model}}</span></td><td data-update="true" data-col="transporter" data-dtype="number" class="dt-number"><span>{{transporter}}</span></td><td data-update="true" data-col="status" data-dtype="number" class="dt-number"><span>{{status}}</span></td><td data-update="true" data-col="request_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{request_timestamp}}</span></td><td data-update="true" data-col="conclusion_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{conclusion_timestamp}}</span></td><td data-update="true" data-col="conclusion_text" data-dtype="text" class="dt-text"><span>{{conclusion_text}}</span></td></tr>{{/each}}
</table>