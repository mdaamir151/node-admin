<table id="content-table" data-key="id" data-update_rows="true" data-table="model_add_requests">
<tr><th>#</th><th>id</th><th>model</th><th>transporter</th><th>status</th><th>request_timestamp</th><th>conclusion_timestamp</th><th>conclusion_text</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="int"><span>{{id}}</span></td><td data-update="true" data-col="model" data-dtype="varchar(60)"><span>{{model}}</span></td><td data-update="true" data-col="transporter" data-dtype="int"><span>{{transporter}}</span></td><td data-update="true" data-col="status" data-dtype="int"><span>{{status}}</span></td><td data-update="true" data-col="request_timestamp" data-dtype="bigint"><span>{{request_timestamp}}</span></td><td data-update="true" data-col="conclusion_timestamp" data-dtype="bigint"><span>{{conclusion_timestamp}}</span></td><td data-update="true" data-col="conclusion_text" data-dtype="text"><span>{{conclusion_text}}</span></td></tr>{{/each}}
</table>