<table id="content-table" data-key="order_id,driver_id" data-update_rows="true" data-table="driver_trips">
<tr><th>#</th><th>order_id</th><th>customer_id</th><th>bidder_id</th><th>driver_id</th><th>status</th><th>start_timestamp</th><th>load_timestamp</th><th>onroad_timestamp</th><th>unload_timestamp</th><th>completion_timestamp</th><th>latitude</th><th>longitude</th><th>vehicle_number</th><th>assign_timestamp</th><th>last_update_timestamp</th><th>receive_payment</th><th>payment_received</th><th>payment_timestamp</th><th>payment_confirmed</th><th>final_payment</th><th>loading_code</th><th>unloading_code</th><th>final_payment_received</th><th>final_payment_timestamp</th><th>final_payment_confirmed</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="order_id" data-dtype="int"><span>{{order_id}}</span></td><td data-update="true" data-col="customer_id" data-dtype="int"><span>{{customer_id}}</span></td><td data-update="true" data-col="bidder_id" data-dtype="int"><span>{{bidder_id}}</span></td><td data-update="true" data-col="driver_id" data-dtype="int"><span>{{driver_id}}</span></td><td data-update="true" data-col="status" data-dtype="int"><span>{{status}}</span></td><td data-update="true" data-col="start_timestamp" data-dtype="bigint"><span>{{start_timestamp}}</span></td><td data-update="true" data-col="load_timestamp" data-dtype="bigint"><span>{{load_timestamp}}</span></td><td data-update="true" data-col="onroad_timestamp" data-dtype="bigint"><span>{{onroad_timestamp}}</span></td><td data-update="true" data-col="unload_timestamp" data-dtype="bigint"><span>{{unload_timestamp}}</span></td><td data-update="true" data-col="completion_timestamp" data-dtype="bigint"><span>{{completion_timestamp}}</span></td><td data-update="true" data-col="latitude" data-dtype="double"><span>{{latitude}}</span></td><td data-update="true" data-col="longitude" data-dtype="double"><span>{{longitude}}</span></td><td data-update="true" data-col="vehicle_number" data-dtype="varchar(30)"><span>{{vehicle_number}}</span></td><td data-update="true" data-col="assign_timestamp" data-dtype="bigint"><span>{{assign_timestamp}}</span></td><td data-update="true" data-col="last_update_timestamp" data-dtype="bigint"><span>{{last_update_timestamp}}</span></td><td data-update="true" data-col="receive_payment" data-dtype="tinyint(1)"><span>{{receive_payment}}</span></td><td data-update="true" data-col="payment_received" data-dtype="int"><span>{{payment_received}}</span></td><td data-update="true" data-col="payment_timestamp" data-dtype="bigint"><span>{{payment_timestamp}}</span></td><td data-update="true" data-col="payment_confirmed" data-dtype="tinyint(1)"><span>{{payment_confirmed}}</span></td><td data-update="true" data-col="final_payment" data-dtype="int"><span>{{final_payment}}</span></td><td data-update="true" data-col="loading_code" data-dtype="int"><span>{{loading_code}}</span></td><td data-update="true" data-col="unloading_code" data-dtype="int"><span>{{unloading_code}}</span></td><td data-update="true" data-col="final_payment_received" data-dtype="int"><span>{{final_payment_received}}</span></td><td data-update="true" data-col="final_payment_timestamp" data-dtype="bigint"><span>{{final_payment_timestamp}}</span></td><td data-update="true" data-col="final_payment_confirmed" data-dtype="tinyint(1)"><span>{{final_payment_confirmed}}</span></td></tr>{{/each}}
</table>