<table id="content-table" data-key="order_id,bidder_id" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="bidding">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="order_id" data-dtype="number" data-default="null" data-nullable="false">order_id</th><th data-insert="true" data-allow_update="true" data-col="bidder_id" data-dtype="number" data-default="null" data-nullable="false">bidder_id</th><th data-insert="true" data-allow_update="true" data-col="amount" data-dtype="number" data-default="null" data-nullable="false">amount</th><th data-insert="true" data-allow_update="true" data-col="customer_id" data-dtype="number" data-default="null" data-nullable="false">customer_id</th><th data-insert="true" data-allow_update="true" data-col="delivery_time" data-dtype="number" data-default="null" data-nullable="false">delivery_time</th><th data-insert="true" data-allow_update="true" data-col="bid_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">bid_timestamp</th><th data-insert="true" data-allow_update="true" data-col="collapsed" data-dtype="boolean" data-default="0" data-nullable="true">collapsed</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="order_id" data-dtype="number" class="dt-number"><span>{{order_id}}</span></td><td data-update="true" data-col="bidder_id" data-dtype="number" class="dt-number"><span>{{bidder_id}}</span></td><td data-update="true" data-col="amount" data-dtype="number" class="dt-number"><span>{{amount}}</span></td><td data-update="true" data-col="customer_id" data-dtype="number" class="dt-number"><span>{{customer_id}}</span></td><td data-update="true" data-col="delivery_time" data-dtype="number" class="dt-number"><span>{{delivery_time}}</span></td><td data-update="true" data-col="bid_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{bid_timestamp}}</span></td><td data-update="true" data-col="collapsed" data-dtype="boolean" class="dt-boolean"><span>{{collapsed}}</span></td></tr>{{/each}}
</table>