<table id="content-table" data-key="order_id,bidder_id" data-update_rows="true" data-table="bidding">
<tr><th>#</th><th>order_id</th><th>bidder_id</th><th>amount</th><th>customer_id</th><th>delivery_time</th><th>bid_timestamp</th><th>collapsed</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="order_id" data-dtype="int"><span>{{order_id}}</span></td><td data-update="true" data-col="bidder_id" data-dtype="int"><span>{{bidder_id}}</span></td><td data-update="true" data-col="amount" data-dtype="int"><span>{{amount}}</span></td><td data-update="true" data-col="customer_id" data-dtype="int"><span>{{customer_id}}</span></td><td data-update="true" data-col="delivery_time" data-dtype="int"><span>{{delivery_time}}</span></td><td data-update="true" data-col="bid_timestamp" data-dtype="bigint"><span>{{bid_timestamp}}</span></td><td data-update="true" data-col="collapsed" data-dtype="tinyint(1)"><span>{{collapsed}}</span></td></tr>{{/each}}
</table>