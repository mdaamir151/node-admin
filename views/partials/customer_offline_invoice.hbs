<table id="content-table" data-key="order_id" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="customer_offline_invoice">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="order_id" data-dtype="number" data-default="null" data-nullable="false">order_id</th><th data-insert="true" data-allow_update="true" data-col="net_amount" data-dtype="number" data-default="null" data-nullable="false">net_amount</th><th data-insert="true" data-allow_update="true" data-col="description" data-dtype="text" data-default="null" data-nullable="true">description</th><th data-insert="true" data-allow_update="true" data-col="transaction_type" data-dtype="number" data-default="null" data-nullable="false">transaction_type</th><th data-insert="true" data-allow_update="true" data-col="payer_id" data-dtype="number" data-default="null" data-nullable="false">payer_id</th><th data-insert="true" data-allow_update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">creation_timestamp</th><th data-insert="true" data-allow_update="true" data-col="gst_rate" data-dtype="text" data-default="null" data-nullable="true">gst_rate</th><th data-insert="true" data-allow_update="true" data-col="discount" data-dtype="text" data-default="0.00" data-nullable="true">discount</th><th data-insert="true" data-allow_update="true" data-col="base_amount" data-dtype="number" data-default="0" data-nullable="true">base_amount</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="order_id" data-dtype="number" class="dt-number"><span>{{order_id}}</span></td><td data-update="true" data-col="net_amount" data-dtype="number" class="dt-number"><span>{{net_amount}}</span></td><td data-update="true" data-col="description" data-dtype="text" class="dt-text"><span>{{description}}</span></td><td data-update="true" data-col="transaction_type" data-dtype="number" class="dt-number"><span>{{transaction_type}}</span></td><td data-update="true" data-col="payer_id" data-dtype="number" class="dt-number"><span>{{payer_id}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="gst_rate" data-dtype="text" class="dt-text"><span>{{gst_rate}}</span></td><td data-update="true" data-col="discount" data-dtype="text" class="dt-text"><span>{{discount}}</span></td><td data-update="true" data-col="base_amount" data-dtype="number" class="dt-number"><span>{{base_amount}}</span></td></tr>{{/each}}
</table>