<table id="content-table" data-key="order_id" data-update_rows="true" data-table="customer_offline_invoice">
<tr><th>#</th><th>order_id</th><th>net_amount</th><th>description</th><th>transaction_type</th><th>payer_id</th><th>creation_timestamp</th><th>gst_rate</th><th>discount</th><th>base_amount</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="order_id" data-dtype="int"><span>{{order_id}}</span></td><td data-update="true" data-col="net_amount" data-dtype="int"><span>{{net_amount}}</span></td><td data-update="true" data-col="description" data-dtype="text"><span>{{description}}</span></td><td data-update="true" data-col="transaction_type" data-dtype="int"><span>{{transaction_type}}</span></td><td data-update="true" data-col="payer_id" data-dtype="int"><span>{{payer_id}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="bigint"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="gst_rate" data-dtype="double(10,2)"><span>{{gst_rate}}</span></td><td data-update="true" data-col="discount" data-dtype="double(10,2)"><span>{{discount}}</span></td><td data-update="true" data-col="base_amount" data-dtype="int"><span>{{base_amount}}</span></td></tr>{{/each}}
</table>