<table id="content-table" data-key="id" data-update_rows="true" data-table="discounts">
<tr><th>#</th><th>id</th><th>coupon_code</th><th>title</th><th>creation_timestamp</th><th>message</th><th>exp_timestamp</th><th>discount_type</th><th>discount_val</th><th>max_cap</th><th>valid</th><th>counter</th><th>max_counter</th><th>user_id</th><th>group_id</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="int"><span>{{id}}</span></td><td data-update="true" data-col="coupon_code" data-dtype="varchar(10)"><span>{{coupon_code}}</span></td><td data-update="true" data-col="title" data-dtype="varchar(50)"><span>{{title}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="bigint"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="message" data-dtype="text"><span>{{message}}</span></td><td data-update="true" data-col="exp_timestamp" data-dtype="bigint"><span>{{exp_timestamp}}</span></td><td data-update="true" data-col="discount_type" data-dtype="tinyint"><span>{{discount_type}}</span></td><td data-update="true" data-col="discount_val" data-dtype="int"><span>{{discount_val}}</span></td><td data-update="true" data-col="max_cap" data-dtype="int"><span>{{max_cap}}</span></td><td data-update="true" data-col="valid" data-dtype="tinyint(1)"><span>{{valid}}</span></td><td data-update="true" data-col="counter" data-dtype="int"><span>{{counter}}</span></td><td data-update="true" data-col="max_counter" data-dtype="int"><span>{{max_counter}}</span></td><td data-update="true" data-col="user_id" data-dtype="int"><span>{{user_id}}</span></td><td data-update="true" data-col="group_id" data-dtype="int"><span>{{group_id}}</span></td></tr>{{/each}}
</table>