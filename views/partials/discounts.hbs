<table id="content-table" data-key="id" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="discounts">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="id" data-dtype="number" data-default="null" data-nullable="false">id</th><th data-insert="true" data-allow_update="true" data-col="coupon_code" data-dtype="text" data-default="null" data-nullable="false">coupon_code</th><th data-insert="true" data-allow_update="true" data-col="title" data-dtype="text" data-default="null" data-nullable="false">title</th><th data-insert="true" data-allow_update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">creation_timestamp</th><th data-insert="true" data-allow_update="true" data-col="message" data-dtype="text" data-default="null" data-nullable="false">message</th><th data-insert="true" data-allow_update="true" data-col="exp_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">exp_timestamp</th><th data-insert="true" data-allow_update="true" data-col="discount_type" data-dtype="number" data-default="null" data-nullable="true">discount_type</th><th data-insert="true" data-allow_update="true" data-col="discount_val" data-dtype="number" data-default="null" data-nullable="true">discount_val</th><th data-insert="true" data-allow_update="true" data-col="max_cap" data-dtype="number" data-default="null" data-nullable="true">max_cap</th><th data-insert="true" data-allow_update="true" data-col="valid" data-dtype="boolean" data-default="1" data-nullable="true">valid</th><th data-insert="true" data-allow_update="true" data-col="counter" data-dtype="number" data-default="0" data-nullable="true">counter</th><th data-insert="true" data-allow_update="true" data-col="max_counter" data-dtype="number" data-default="null" data-nullable="false">max_counter</th><th data-insert="true" data-allow_update="true" data-col="user_id" data-dtype="number" data-default="0" data-nullable="true">user_id</th><th data-insert="true" data-allow_update="true" data-col="group_id" data-dtype="number" data-default="1" data-nullable="true">group_id</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="number" class="dt-number"><span>{{id}}</span></td><td data-update="true" data-col="coupon_code" data-dtype="text" class="dt-text"><span>{{coupon_code}}</span></td><td data-update="true" data-col="title" data-dtype="text" class="dt-text"><span>{{title}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="message" data-dtype="text" class="dt-text"><span>{{message}}</span></td><td data-update="true" data-col="exp_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{exp_timestamp}}</span></td><td data-update="true" data-col="discount_type" data-dtype="number" class="dt-number"><span>{{discount_type}}</span></td><td data-update="true" data-col="discount_val" data-dtype="number" class="dt-number"><span>{{discount_val}}</span></td><td data-update="true" data-col="max_cap" data-dtype="number" class="dt-number"><span>{{max_cap}}</span></td><td data-update="true" data-col="valid" data-dtype="boolean" class="dt-boolean"><span>{{valid}}</span></td><td data-update="true" data-col="counter" data-dtype="number" class="dt-number"><span>{{counter}}</span></td><td data-update="true" data-col="max_counter" data-dtype="number" class="dt-number"><span>{{max_counter}}</span></td><td data-update="true" data-col="user_id" data-dtype="number" class="dt-number"><span>{{user_id}}</span></td><td data-update="true" data-col="group_id" data-dtype="number" class="dt-number"><span>{{group_id}}</span></td></tr>{{/each}}
</table>