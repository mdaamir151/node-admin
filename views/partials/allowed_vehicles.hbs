<table id="content-table" data-key="id" data-update_rows="true" data-delete_rows="false" data-insert_rows="true" data-table="allowed_vehicles">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="id" data-dtype="text" data-default="null" data-nullable="false">id</th><th data-insert="true" data-allow_update="true" data-col="brand" data-dtype="text" data-default="null" data-nullable="false">brand</th><th data-insert="true" data-allow_update="true" data-col="name" data-dtype="text" data-default="null" data-nullable="false">name</th><th data-insert="true" data-allow_update="true" data-col="image_path" data-dtype="text" data-default="null" data-nullable="true">image_path</th><th data-insert="true" data-allow_update="true" data-col="payload_kgs" data-dtype="number" data-default="0" data-nullable="true">payload_kgs</th><th data-insert="true" data-allow_update="true" data-col="category" data-dtype="text" data-default="null" data-nullable="true">category</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="id" data-dtype="text" class="dt-text"><span>{{id}}</span></td><td data-update="true" data-col="brand" data-dtype="text" class="dt-text"><span>{{brand}}</span></td><td data-update="true" data-col="name" data-dtype="text" class="dt-text"><span>{{name}}</span></td><td data-update="true" data-col="image_path" data-dtype="text" class="dt-text"><span>{{image_path}}</span></td><td data-update="true" data-col="payload_kgs" data-dtype="number" class="dt-number"><span>{{payload_kgs}}</span></td><td data-update="true" data-col="category" data-dtype="text" class="dt-text"><span>{{category}}</span></td></tr>{{/each}}
</table>