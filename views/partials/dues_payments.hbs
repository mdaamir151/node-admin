<table id="content-table" data-key="transaction_id" data-update_rows="true" data-table="dues_payments">
<tr><th>#</th><th>transaction_id</th><th>paid_amount</th><th>payment_id</th><th>payment_timestamp</th><th>payer_id</th><th>paid_by</th><th>creation_timestamp</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="transaction_id" data-dtype="varchar(80)"><span>{{transaction_id}}</span></td><td data-update="true" data-col="paid_amount" data-dtype="int"><span>{{paid_amount}}</span></td><td data-update="true" data-col="payment_id" data-dtype="varchar(80)"><span>{{payment_id}}</span></td><td data-update="true" data-col="payment_timestamp" data-dtype="bigint"><span>{{payment_timestamp}}</span></td><td data-update="true" data-col="payer_id" data-dtype="int"><span>{{payer_id}}</span></td><td data-update="true" data-col="paid_by" data-dtype="int"><span>{{paid_by}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="bigint"><span>{{creation_timestamp}}</span></td></tr>{{/each}}
</table>