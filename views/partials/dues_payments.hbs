<table id="content-table" data-key="transaction_id" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="dues_payments">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="transaction_id" data-dtype="text" data-default="null" data-nullable="false">transaction_id</th><th data-insert="true" data-allow_update="true" data-col="paid_amount" data-dtype="number" data-default="null" data-nullable="false">paid_amount</th><th data-insert="true" data-allow_update="true" data-col="payment_id" data-dtype="text" data-default="null" data-nullable="false">payment_id</th><th data-insert="true" data-allow_update="true" data-col="payment_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">payment_timestamp</th><th data-insert="true" data-allow_update="true" data-col="payer_id" data-dtype="number" data-default="null" data-nullable="false">payer_id</th><th data-insert="true" data-allow_update="true" data-col="paid_by" data-dtype="number" data-default="null" data-nullable="false">paid_by</th><th data-insert="true" data-allow_update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">creation_timestamp</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="transaction_id" data-dtype="text" class="dt-text"><span>{{transaction_id}}</span></td><td data-update="true" data-col="paid_amount" data-dtype="number" class="dt-number"><span>{{paid_amount}}</span></td><td data-update="true" data-col="payment_id" data-dtype="text" class="dt-text"><span>{{payment_id}}</span></td><td data-update="true" data-col="payment_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{payment_timestamp}}</span></td><td data-update="true" data-col="payer_id" data-dtype="number" class="dt-number"><span>{{payer_id}}</span></td><td data-update="true" data-col="paid_by" data-dtype="number" class="dt-number"><span>{{paid_by}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{creation_timestamp}}</span></td></tr>{{/each}}
</table>