<table id="content-table" data-key="transaction_id" data-update_rows="true" data-delete_rows="false" data-insert_rows="false" data-table="customer_payments">
<tr><th data-col="serial-no" data-insert="false">#</th><th data-insert="true" data-allow_update="true" data-col="transaction_id" data-dtype="text" data-default="null" data-nullable="false">transaction_id</th><th data-insert="true" data-allow_update="true" data-col="paid_amount" data-dtype="number" data-default="null" data-nullable="false">paid_amount</th><th data-insert="true" data-allow_update="true" data-col="order_id" data-dtype="number" data-default="null" data-nullable="false">order_id</th><th data-insert="true" data-allow_update="true" data-col="payment_id" data-dtype="text" data-default="null" data-nullable="false">payment_id</th><th data-insert="true" data-allow_update="true" data-col="description" data-dtype="text" data-default="null" data-nullable="true">description</th><th data-insert="true" data-allow_update="true" data-col="transaction_type" data-dtype="number" data-default="null" data-nullable="false">transaction_type</th><th data-insert="true" data-allow_update="true" data-col="payment_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">payment_timestamp</th><th data-insert="true" data-allow_update="true" data-col="status" data-dtype="number" data-default="0" data-nullable="true">status</th><th data-insert="true" data-allow_update="true" data-col="related_transaction_id" data-dtype="text" data-default="null" data-nullable="true">related_transaction_id</th><th data-insert="true" data-allow_update="true" data-col="remark" data-dtype="text" data-default="null" data-nullable="true">remark</th><th data-insert="true" data-allow_update="true" data-col="payer_id" data-dtype="number" data-default="null" data-nullable="false">payer_id</th><th data-insert="true" data-allow_update="true" data-col="refund_id" data-dtype="text" data-default="null" data-nullable="true">refund_id</th><th data-insert="true" data-allow_update="true" data-col="refund_amount" data-dtype="number" data-default="0" data-nullable="true">refund_amount</th><th data-insert="true" data-allow_update="true" data-col="refund_description" data-dtype="text" data-default="null" data-nullable="true">refund_description</th><th data-insert="true" data-allow_update="true" data-col="cancellation_charge" data-dtype="number" data-default="0" data-nullable="true">cancellation_charge</th><th data-insert="true" data-allow_update="true" data-col="cancelled_by" data-dtype="number" data-default="0" data-nullable="true">cancelled_by</th><th data-insert="true" data-allow_update="true" data-col="cancellation_timestamp" data-dtype="unixtimestamp" data-default="0" data-nullable="true">cancellation_timestamp</th><th data-insert="true" data-allow_update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" data-default="null" data-nullable="false">creation_timestamp</th><th data-insert="true" data-allow_update="true" data-col="gst_rate" data-dtype="text" data-default="null" data-nullable="true">gst_rate</th><th data-insert="true" data-allow_update="true" data-col="discount" data-dtype="text" data-default="0.00" data-nullable="true">discount</th><th data-insert="true" data-allow_update="true" data-col="base_amount" data-dtype="number" data-default="0" data-nullable="true">base_amount</th></tr>
{{#each data}}<tr><td class="dt-number" class="serial-no">{{increment @index}}</td><td data-update="true" data-col="transaction_id" data-dtype="text" class="dt-text"><span>{{transaction_id}}</span></td><td data-update="true" data-col="paid_amount" data-dtype="number" class="dt-number"><span>{{paid_amount}}</span></td><td data-update="true" data-col="order_id" data-dtype="number" class="dt-number"><span>{{order_id}}</span></td><td data-update="true" data-col="payment_id" data-dtype="text" class="dt-text"><span>{{payment_id}}</span></td><td data-update="true" data-col="description" data-dtype="text" class="dt-text"><span>{{description}}</span></td><td data-update="true" data-col="transaction_type" data-dtype="number" class="dt-number"><span>{{transaction_type}}</span></td><td data-update="true" data-col="payment_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{payment_timestamp}}</span></td><td data-update="true" data-col="status" data-dtype="number" class="dt-number"><span>{{status}}</span></td><td data-update="true" data-col="related_transaction_id" data-dtype="text" class="dt-text"><span>{{related_transaction_id}}</span></td><td data-update="true" data-col="remark" data-dtype="text" class="dt-text"><span>{{remark}}</span></td><td data-update="true" data-col="payer_id" data-dtype="number" class="dt-number"><span>{{payer_id}}</span></td><td data-update="true" data-col="refund_id" data-dtype="text" class="dt-text"><span>{{refund_id}}</span></td><td data-update="true" data-col="refund_amount" data-dtype="number" class="dt-number"><span>{{refund_amount}}</span></td><td data-update="true" data-col="refund_description" data-dtype="text" class="dt-text"><span>{{refund_description}}</span></td><td data-update="true" data-col="cancellation_charge" data-dtype="number" class="dt-number"><span>{{cancellation_charge}}</span></td><td data-update="true" data-col="cancelled_by" data-dtype="number" class="dt-number"><span>{{cancelled_by}}</span></td><td data-update="true" data-col="cancellation_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{cancellation_timestamp}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="unixtimestamp" class="dt-unixtimestamp"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="gst_rate" data-dtype="text" class="dt-text"><span>{{gst_rate}}</span></td><td data-update="true" data-col="discount" data-dtype="text" class="dt-text"><span>{{discount}}</span></td><td data-update="true" data-col="base_amount" data-dtype="number" class="dt-number"><span>{{base_amount}}</span></td></tr>{{/each}}
</table>