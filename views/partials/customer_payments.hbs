<table id="content-table" data-key="transaction_id" data-update_rows="true" data-table="customer_payments">
<tr><th>#</th><th>transaction_id</th><th>paid_amount</th><th>order_id</th><th>payment_id</th><th>description</th><th>transaction_type</th><th>payment_timestamp</th><th>status</th><th>related_transaction_id</th><th>remark</th><th>payer_id</th><th>refund_id</th><th>refund_amount</th><th>refund_description</th><th>cancellation_charge</th><th>cancelled_by</th><th>cancellation_timestamp</th><th>creation_timestamp</th><th>gst_rate</th><th>discount</th><th>base_amount</th></tr>
{{#each data}}<tr><td>{{increment @index}}</td><td data-update="true" data-col="transaction_id" data-dtype="varchar(80)"><span>{{transaction_id}}</span></td><td data-update="true" data-col="paid_amount" data-dtype="int"><span>{{paid_amount}}</span></td><td data-update="true" data-col="order_id" data-dtype="int"><span>{{order_id}}</span></td><td data-update="true" data-col="payment_id" data-dtype="varchar(80)"><span>{{payment_id}}</span></td><td data-update="true" data-col="description" data-dtype="text"><span>{{description}}</span></td><td data-update="true" data-col="transaction_type" data-dtype="int"><span>{{transaction_type}}</span></td><td data-update="true" data-col="payment_timestamp" data-dtype="bigint"><span>{{payment_timestamp}}</span></td><td data-update="true" data-col="status" data-dtype="tinyint"><span>{{status}}</span></td><td data-update="true" data-col="related_transaction_id" data-dtype="varchar(80)"><span>{{related_transaction_id}}</span></td><td data-update="true" data-col="remark" data-dtype="text"><span>{{remark}}</span></td><td data-update="true" data-col="payer_id" data-dtype="int"><span>{{payer_id}}</span></td><td data-update="true" data-col="refund_id" data-dtype="varchar(80)"><span>{{refund_id}}</span></td><td data-update="true" data-col="refund_amount" data-dtype="int"><span>{{refund_amount}}</span></td><td data-update="true" data-col="refund_description" data-dtype="text"><span>{{refund_description}}</span></td><td data-update="true" data-col="cancellation_charge" data-dtype="int"><span>{{cancellation_charge}}</span></td><td data-update="true" data-col="cancelled_by" data-dtype="tinyint"><span>{{cancelled_by}}</span></td><td data-update="true" data-col="cancellation_timestamp" data-dtype="bigint"><span>{{cancellation_timestamp}}</span></td><td data-update="true" data-col="creation_timestamp" data-dtype="bigint"><span>{{creation_timestamp}}</span></td><td data-update="true" data-col="gst_rate" data-dtype="double(10,2)"><span>{{gst_rate}}</span></td><td data-update="true" data-col="discount" data-dtype="double(10,2)"><span>{{discount}}</span></td><td data-update="true" data-col="base_amount" data-dtype="int"><span>{{base_amount}}</span></td></tr>{{/each}}
</table>